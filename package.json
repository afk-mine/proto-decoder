{
  "name": "proto-decoder",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run --hot src/server.ts",
    "generate:mysql": "drizzle-kit generate:mysql --schema=./src/db/schema",
    "migrate:mesages": "bun run src/migrateFromFileToDB.ts",
    "start": "bun run src/server.ts"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "arrowParens": "always",
    "endOfLine": "lf"
  },
  "devDependencies": {
    "bun-types": "latest",
    "drizzle-kit": "^0.19.13",
    "prettier": "^3.0.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/cron": "^0.7.0",
    "crc-32": "^1.2.2",
    "drizzle-orm": "^0.28.6",
    "elysia": "^0.7.29",
    "mysql2": "^3.6.2",
    "protobufjs": "^7.2.5",
    "tslog": "^4.9.2"
  }
}
